{
    "compilerOptions": {
        "baseUrl": "src",
        "paths": {
            "Style/*": ["style/*"],
            "@component/*": ["component/*"],
            "Route/*": ["route/*"],
            "Store/*": ["store/*"],
            "Util/*": ["util/*"],
            "Query/*": ["query/*"],
            "Type/*": ["type/*"],
            "@hook/*": ["hook/*"],
            "Graphql/*": ["graphql/*"],
            "Ui/*": ["ui/*"],
            "Dispatcher/*": ["dispatcher/*"]
        }
    },
    "exclude": [
        "node_modules",
        ".next"
    ]
}
